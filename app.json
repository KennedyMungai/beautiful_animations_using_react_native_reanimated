{
  "expo": {
    "name": "beautiful_animations",
    "slug": "beautiful_animations",
    "version": "1.0.0",
    "scheme": "beautiful_animations",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kennedymungai.beautiful_animations"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "cd4094f6-c777-497a-95d0-2b541d4de538"
      }
    }
  }
}
